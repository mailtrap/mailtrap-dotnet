{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "Warning",
            "Microsoft": "Warning"
        },
        "Debug": {
            "LogLevel": {
                "Default": "Debug"
            }
        }
    },
    "Mailtrap": {
        "Authentication": {
            "ApiToken": "<API_KEY>"
        },
        "SendEndpoint": {
            "BaseUrl": "https://api.mailtrap.io/v1/send"
        },
        "Serialization": {
            "PrettyJson": true
        }
    }

}
